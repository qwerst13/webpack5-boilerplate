{
  "name": "webpack5-boilerplate",
  "version": "1.0.0",
  "description": "Basic boilerplate made with webpack5",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "dev": "webpack",
    "start": "webpack serve",
    "eslint": "eslint src/js/*",
    "eslint:--fix": "eslint --fix src/js/*",
    "fix": "npm run eslint:--fix && npm run stylelint:--fix",
    "stylelint": "stylelint ./**/*.css !./dist/**/*.css",
    "stylelint:--fix": "stylelint ./**/*.css !./dist/**/*.css --fix"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qwerst13/webpack5-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/qwerst13/webpack5-boilerplate/issues"
  },
  "homepage": "https://github.com/qwerst13/webpack5-boilerplate#readme",
  "browserslist": [
    "defaults"
  ],
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.8",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^2.1.0",
    "postcss": "^8.3.6",
    "postcss-loader": "^6.1.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.2",
    "sass": "^1.36.0",
    "sass-loader": "^12.1.0",
    "stylelint": "^13.13.1",
    "webpack": "^5.47.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  }
}
